{
    "name": "Turntable",
    "version": "0.1.1-beta",
    "license": "MIT",
    "description": "Simple youtube DJ app made with Electron and Angular4",
    "homepage": "https://github.com/radiium/turntable",
    "repository": {
        "type": "git",
        "url": "https://github.com/radiium/turntable.git"
    },
    "bugs": {
        "url": "https://github.com/radiium/turntable/issues",
        "email": "remip.dev@gmail.com"
    },
    "author": {
        "name": "radiium",
        "email": "remip.dev@gmail.com",
        "url": "https://github.com/radiium"
    },
    "keywords": [
        "electron",
        "youtube",
        "youtubeplayer",
        "dj",
        "angular",
        "javascript",
        "typescript"
    ],
    "private": true,
    "main": "dist/main.js",
    "scripts": {
        "electrontsc": "tsc -p electron/",
        "lint": " tslint --type-check --project tsconfig.json src/**/*.ts",
        "start": "ng serve",
        "test": " ng test",
        "e2e": "  ng e2e",
        "prepare": "        ng build        && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
        "prepare:dev": "    ng build        && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
        "prepare:prod": "   ng build --prod && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
        "electron": "       npm run electron:dev",
        "electron:dev": "   npm run prepare:dev && electron dist",
        "electron:prod": "  npm run prepare:prod && electron dist",
        "package:dev": "    npm run prepare:dev  && electron-packager dist --out=release --overwrite",
        "package:prod": "   npm run prepare:prod && electron-packager dist --out=release --asar=true --overwrite",
        "package": "        npm run package:dev",
        "release": "        npm run package:prod && build",
        "pack": "           electron-builder -lmw --config electron-builder.yml --dir",
        "dist": "           electron-builder -lmw --config electron-builder.yml"
    },
    "dependencies": {
        "@angular/animations": "^4.4.3",
        "@angular/cdk": "^2.0.0-beta.11",
        "@angular/common": "^4.4.3",
        "@angular/compiler": "^4.4.3",
        "@angular/core": "^4.4.3",
        "@angular/forms": "^4.4.3",
        "@angular/http": "^4.4.3",
        "@angular/material": "^2.0.0-beta.11",
        "@angular/platform-browser": "^4.4.3",
        "@angular/platform-browser-dynamic": "^4.4.3",
        "@angular/router": "^4.4.3",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.4",
        "angular2-fontawesome": "^0.9.3",
        "angular2-uuid": "^1.1.1",
        "bootstrap": "^3.3.7",
        "core-js": "^2.5.1",
        "electron-json-storage": "^3.1.0",
        "font-awesome": "^4.7.0",
        "hammerjs": "^2.0.8",
        "moment": "^2.18.1",
        "ng2-dragula": "^1.5.0",
        "ngx-electron": "^1.0.3",
        "rxjs": "^5.4.2",
        "youtube": "^0.1.0",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/cli": "^1.4.3",
        "@angular/compiler-cli": "^4.4.3",
        "@angular/language-service": "^4.4.3",
        "@types/jasmine": "^2.6.0",
        "@types/jasminewd2": "^2.0.3",
        "@types/node": "^8.0.31",
        "@types/youtube": "0.0.31",
        "codelyzer": "^3.2.0",
        "electron": "^1.7.8",
        "electron-builder": "^19.31.1",
        "electron-builder-squirrel-windows": "^19.30.3",
        "electron-debug": "^1.4.0",
        "jasmine-core": "^2.8.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ncp": "^2.0.0",
        "node-sass": "^4.5.3",
        "path": "^0.12.7",
        "protractor": "~5.1.2",
        "ts-node": "^3.3.0",
        "tslint": "^5.7.0",
        "typescript": "^2.3.4",
        "url": "^0.11.0"
    }
}
