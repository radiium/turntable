{
    "name": "Turntable",
    "version": "0.2.2-beta",
    "license": "MIT",
    "description": "Multiplatform Youtube DJ app made with Electron and Angular4",
    "homepage": "https://github.com/radiium/turntable",
    "repository": {
        "type": "git",
        "url": "https://github.com/radiium/turntable.git"
    },
    "bugs": {
        "url": "https://github.com/radiium/turntable/issues",
        "email": "remip.dev@gmail.com"
    },
    "author": {
        "name": "radiium",
        "email": "remip.dev@gmail.com",
        "url": "https://github.com/radiium"
    },
    "keywords": [
        "electron",
        "youtube",
        "youtubeplayer",
        "dj",
        "angular",
        "javascript",
        "typescript"
    ],
    "private": true,
    "main": "dist/main.js",
    "scripts": {
        "electrontsc": "    tsc -p electron/",
        "lint": "           tslint --type-check --project tsconfig.json 'src/**/*.ts'",
        "start": "          ng serve",
        "test": "           ng test",
        "e2e": "            ng e2e",
        "prepare": "        ng build        && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
        "prepare:dev": "    ng build        && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
        "prepare:prod": "   ng build --prod && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
        "electron": "       npm run electron:dev",
        "electron:dev": "   npm run prepare:dev && electron dist",
        "electron:prod": "  npm run prepare:prod && electron dist",
        "package:dev": "    npm run prepare:dev  && electron-packager dist --out=release --overwrite",
        "package:prod": "   npm run prepare:prod && electron-packager dist --out=release --asar=true --overwrite",
        "package": "        npm run package:dev",
        "release": "        npm run package:prod && build",
        "pack": "           electron-builder -lmw --config electron-builder.yml --dir",
        "dist": "           electron-builder -lmw --config electron-builder.yml"
    },
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/cdk": "^5.0.1",
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/http": "^5.0.0",
        "@angular/material": "^5.0.1",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/router": "^5.0.0",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.4",
        "@ngx-translate/core": "^9.0.1",
        "angular2-fontawesome": "^0.9.3",
        "angular2-uuid": "^1.1.1",
        "bootstrap": "^3.3.7",
        "core-js": "^2.4.1",
        "dom-autoscroller": "^2.3.3",
        "electron-json-storage": "^4.0.2",
        "font-awesome": "^4.7.0",
        "hammerjs": "^2.0.8",
        "moment": "^2.19.4",
        "ng2-dragula": "^1.5.0",
        "ngx-electron": "^1.0.4",
        "rxjs": "^5.5.2",
        "youtube": "^0.1.0",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/cli": "^1.6.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/language-service": "^5.0.0",
        "@types/jasmine": "^2.5.53",
        "@types/jasminewd2": "^2.0.2",
        "@types/node": "^6.0.60",
        "@types/youtube": "0.0.31",
        "codelyzer": "^4.0.1",
        "del": "^3.0.0",
        "electron": "^1.7.9",
        "electron-builder": "^19.49.0",
        "electron-builder-squirrel-windows": "^19.49.0",
        "jasmine-core": "^2.6.2",
        "jasmine-spec-reporter": "^4.1.0",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ncp": "^2.0.0",
        "node-sass": "^4.7.2",
        "path": "^0.12.7",
        "protractor": "^5.1.2",
        "ts-node": "^3.2.0",
        "tslint": "^5.7.0",
        "typescript": "^2.4.2",
        "url": "^0.11.0"
    }
}
