{
    "name": "Turntable",
    "version": "0.1.1-beta",
    "license": "MIT",
    "description": "Simple youtube DJ app made with Electron and Angular4",
    "homepage": "https://github.com/radiium/turntable",
    "repository": {
        "type": "git",
        "url": "https://github.com/radiium/turntable.git"
    },
    "bugs": {
        "url": "https://github.com/radiium/turntable/issues",
        "email": "remip.dev@gmail.com"
    },
    "author": {
        "name": "radiium",
        "email": "remip.dev@gmail.com",
        "url": "https://github.com/radiium"
    },
    "keywords": [
        "electron",
        "youtube",
        "youtubeplayer",
        "dj",
        "angular",
        "javascript",
        "typescript"
    ],
    "private": true,
    "main": "dist/main.js",
    "scripts": {
        "electrontsc": "tsc -p electron/",
        "lint": " tslint --type-check --project tsconfig.json src/**/*.ts",
        "start": "ng serve",
        "test": " ng test",
        "e2e": "  ng e2e",
        "prepare": "        ng build        && tsc -p electron/ && ncp electron/package.json dist/package.json && ncp electron/settings/settings.html dist/settings/settings.html",
        "prepare:dev": "    ng build        && tsc -p electron/ && ncp electron/package.json dist/package.json && ncp electron/settings/settings.html dist/settings/settings.html",
        "prepare:prod": "   ng build --prod && tsc -p electron/ && ncp electron/package.json dist/package.json && ncp electron/settings/settings.html dist/settings/settings.html",
        "electron": "       npm run electron:dev",
        "electron:dev": "   npm run prepare:dev && electron dist",
        "electron:prod": "  npm run prepare:prod && electron dist",
        "package:dev": "    npm run prepare:dev  && electron-packager dist --out=release --overwrite",
        "package:prod": "   npm run prepare:prod && electron-packager dist --out=release --asar=true --overwrite",
        "package": "        npm run package:dev",
        "release": "        npm run package:prod && build",
        "pack": "           electron-builder -lmw --config electron-builder.yml --dir",
        "dist": "           electron-builder -lmw --config electron-builder.yml"
    },
    "dependencies": {
        "@angular/animations": "^4.2.4",
        "@angular/common": "^4.2.4",
        "@angular/compiler": "^4.2.4",
        "@angular/core": "^4.2.4",
        "@angular/forms": "^4.2.4",
        "@angular/http": "^4.2.4",
        "@angular/platform-browser": "^4.2.4",
        "@angular/platform-browser-dynamic": "^4.2.4",
        "@angular/router": "^4.2.4",
        "@types/youtube": "0.0.29",
        "angular2-fontawesome": "^0.9.3",
        "core-js": "^2.4.1",
        "font-awesome": "^4.7.0",
        "moment": "^2.18.1",
        "ng2-dragula": "^1.5.0",
        "ng2-youtube-player": "0.0.3",
        "rxjs": "^5.4.2",
        "zone.js": "^0.8.14"
    },
    "devDependencies": {
        "@angular/cli": "1.3.2",
        "@angular/compiler-cli": "^4.2.4",
        "@angular/language-service": "^4.2.4",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "codelyzer": "~3.1.1",
        "electron": "^1.7.5",
        "electron-builder": "^19.27.7",
        "electron-builder-squirrel-windows": "^19.28.0",
        "electron-debug": "^1.4.0",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ncp": "^2.0.0",
        "path": "^0.12.7",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.3.2",
        "typescript": "~2.3.3",
        "url": "^0.11.0"
    }
}
