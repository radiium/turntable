{
  "name": "Turntable",
  "version": "1.2.0-beta",
  "license": "MIT",
  "description": "Multiplatform Youtube DJ app made with Electron and Angular 6",
  "homepage": "https://github.com/radiium/turntable",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiium/turntable.git"
  },
  "bugs": {
    "url": "https://github.com/radiium/turntable/issues",
    "email": "remip.dev@gmail.com"
  },
  "author": {
    "name": "radiium",
    "email": "remip.dev@gmail.com",
    "url": "https://github.com/radiium"
  },
  "keywords": [
    "electron",
    "youtube",
    "youtubeplayer",
    "dj",
    "angular 6",
    "javascript",
    "typescript"
  ],
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "electrontsc": "    tsc -p electron/",
    "lint": "           ng lint",
    "start": "          ng serve",
    "test": "           ng test",
    "e2e": "            ng e2e",
    "prepare": "        ng build && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json && ncp src-electron/postcss.config.js dist/postcss.config.js",
    "prepare:dev": "    ng build && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json && ncp src-electron/postcss.config.js dist/postcss.config.js",
    "prepare:prod": "   ng build --prod && tsc -p src-electron/ && ncp src-electron/package.json dist/package.json",
    "electron": "       npm run electron:dev",
    "electron:dev": "   npm run prepare:dev && electron dist",
    "electron:prod": "  npm run prepare:prod && electron dist",
    "package:dev": "    npm run prepare:dev  && electron-packager dist --out=release --overwrite",
    "package:prod": "   npm run prepare:prod && electron-packager dist --out=release --asar=true --overwrite",
    "package": "        npm run package:dev",
    "release": "        npm run package:prod && build",
    "pack": "           electron-builder -lmw --config electron-builder.yml --dir",
    "dist": "           electron-builder -lmw --config electron-builder.yml"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
