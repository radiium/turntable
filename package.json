    "rxjs-compat": "^6.1.0",
    "youtube": "^0.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.1",
    "@angular/cli": "^6.0.1",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/node": "^10.1.0",
    "@types/youtube": "0.0.32",
    "codelyzer": "^4.3.0",
    "del": "^3.0.0",
    "electron": "^2.0.0",
    "electron-builder": "^20.13.4",
    "electron-builder-squirrel-windows": "^20.13.1",
    "jasmine-core": "^3.1.0",
