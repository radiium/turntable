    "ngx-infinite-scroll": "^6.0.0",
    "ngx-spinner": "^6.0.0",
    "npm": "^6.1.0",
    "readline": "^1.3.0",
    "rxjs-compat": "^6.2.0",
    "youtube": "^0.1.0",
    "ytdl-core": "^0.20.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.5",
    "@angular/cli": "^6.0.5",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/node": "^10.1.3",
