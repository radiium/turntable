    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/lodash": "^4.14.109",
    "angular2-fontawesome": "^5.2.1",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.5.7",
    "dom-autoscroller": "^2.3.4",
    "electron-context-menu": "^0.9.1",
    "electron-json-storage": "^4.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
